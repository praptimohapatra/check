package com.hw;

public class RangePrime {
	static int count=0;

	public static void main(String[] args) {
       primeCheck(10,30);
       if(count==2)
       {
    	   System.out.println("The prime numbers are: ");
       }
   

	}
	public static void primeCheck(int n,int i)
	{
	  
	   
		if(i<=n)
		{
			if(n%i==0)
			{
				count++;
			}
			i++;
			primeCheck(n,i);
		}
	   
	}


}
