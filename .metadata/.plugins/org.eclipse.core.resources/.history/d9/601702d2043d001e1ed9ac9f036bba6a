package com.hw;

public class RangePrime {
	static int count=0;

	public static void main(String[] args) {
       primeCheck(7,1);
       if(count==2)
       {
    	   System.out.println("The Number is prime ");
       }
       else
       {
    	   System.out.println("The number is prime");
       }

	}
	public static int primeCheck(int n,int i)
	{
	
		if(i<=n)
		{
			if(n%i==0)
			{
				count++;
			}
			i++;
			primecheck(x,i);
		}
	}



}
